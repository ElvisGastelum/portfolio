{
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.tabSize": 2,
    "files.autoSave": "afterDelay",
    "emmet.includeLanguages": {
        "vue-html":
        "html",
        "javascript":
        "javascriptreact"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}